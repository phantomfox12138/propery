<resources>
    <declare-styleable name="TcSpringFloatingActionMenu">
        <attr name="tc_sfam_colorPressed" format="color" />
        <attr name="tc_sfam_colorNormal" format="color" />
        <attr name="tc_sfam_colorRipple" format="color" />
        <attr name="tc_sfam_colorDisabled" format="color" />
        <attr name="tc_sfam_shadow" format="boolean" />
        <attr name="tc_sfam_size" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <attr name="tc_sfam_position" format="enum">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <attr name="tc_sfam_icon" format="reference" />
        <attr name="tc_sfam_reveal_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TCMenuItemView">
        <attr name="tc_miv_diameter" format="dimension" />
        <attr name="tc_miv_color" format="color" />
        <attr name="tc_miv_icon" format="reference" />
        <attr name="tc_miv_text" format="string" />
        <attr name="tc_miv_textColor" format="color" />
        <attr name="tc_miv_textSize" format="dimension" />
    </declare-styleable>
</resources>